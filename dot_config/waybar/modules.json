    // Hyprland Window
    "hyprland/window": {
        "rewrite": {
            "(.*) - Zen Browser": "$1",
            "(.*) - Brave": "$1",
            "(.*) - Chromium": "$1",
            "(.*) - Brave Search": "$1",
            "(.*) - Outlook": "$1",
            "(.*) Microsoft Teams": "$1"
